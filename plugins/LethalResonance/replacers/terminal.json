{
"condition": {
        "type": "config",
        "config": "EnabledSounds:terminal"
    },
    "randomness": {
        "type": "determinstic"
    },
    "replacements": [
        {
            "matches": "TerminalAudio:EnterTerminal",
            "sounds": [
                {
                    "sound": "terminal/EnterTerminal.ogg"
                }
            ]
        },
        {
            "matches": "TerminalAudio:ExitTerminal",
            "sounds": [
                {
                    "sound": "terminal/ExitTerminal.ogg"
                }
            ]
        },
        {
            "matches": "UIAudio:SignalTranslatorBegin",
            "sounds": [
                {
                    "sound": "terminal/SignalTranslatorBegin.ogg"
                }
            ]
        },
        {
            "matches": "UIAudio:SignalTranslatorFinish",
            "sounds": [
                {
                    "sound": "terminal/SignalTranslatorFinish.ogg"
                }
            ]
        },
        {
            "matches": "PlacementCollider:SignalTranslatorLocalSFX",
            "sounds": [
                {
                    "sound": "terminal/SignalTranslatorLocalSFX.ogg"
                }
            ]
        },
        {
            "matches": "TerminalAudio:TerminalAlarm",
			"condition": {
				"type": "config",
				"config": "A_Options:Performance_Mode_Extras",
				"value": false
			},
            "sounds": [
                {
                    "sound": "terminal/TerminalAlarm.ogg"
                }
            ]
        },
        {
            "matches": "TerminalAudio:TerminalLoadImage",
            "sounds": [
                {
                    "sound": "terminal/TerminalLoadImage.ogg"
                }
            ]
        },
        {
            "matches": "UIAudio:TipsHUD",
            "sounds": [
                {
                    "sound": "terminal/TipsHUD.ogg"
                }
            ]
        },
        {
            "matches": "UIAudio:TipsHUD2",
            "sounds": [
                {
                    "sound": "terminal/TipsHUD2.ogg"
                }
            ]
        }
    ]
}